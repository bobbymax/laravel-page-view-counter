{
    "name": "cyrildewit/eloquent-visitable",
    "description": "Simple package for adding a page visit counter to your visitable Eloquent models",
    "homepage": "https://github.com/cyrildewit/eloquent-visitable",
    "keywords": [
        "cyrildewit",
        "laravel",
        "visits",
        "hits",
        "counter",
        "model"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Cyril de Wit",
            "email": "info@cyrildewit.nl",
            "homepage": "http://cyrildewit.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/contracts": "5.5.*|5.6.*",
        "illuminate/database": "5.5.*|5.6.*",
        "illuminate/support": "5.5.*|5.6.*",
        "nesbot/carbon": "~1.22"
    },
    "require-dev": {
        "orchestra/database": "^3.5",
        "orchestra/testbench": "^3.4",
        "phpunit/phpunit": "^6.5",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "CyrildeWit\\EloquentVisitable\\": "src/EloquentVisitable/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CyrildeWit\\EloquentVisitable\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "CyrildeWit\\EloquentVisitable\\EloquentVisitableServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
